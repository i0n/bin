# Some useful Unicode entities
# Non-Breaking Space
snippet space
	&nbsp;${1}
# ←
snippet left
	&#x2190;${1}
# →
snippet right
	&#x2192;${1}
# ↑
snippet up
	&#x2191;${1}
# ↓
snippet down
	&#x2193;${1}
# ↩
snippet return
	&#x21A9;${1}
# ⇤
snippet backtab
	&#x21E4;${1}
# ⇥
snippet tab
	&#x21E5;${1}
# ⇧
snippet shift
	&#x21E7;${1}
# ⌃
snippet control
	&#x2303;${1}
# ⌅
snippet enter
	&#x2305;${1}
# ⌘
snippet command
	&#x2318;${1}
# ⌥
snippet option_key
	&#x2325;${1}
# ⌦
snippet delete
	&#x2326;${1}
# ⌫
snippet backspace
	&#x232B;${1}
# ⎋
snippet escape
	&#x238B;${1}

snippet &
	&amp;${1}
# &
snippet copyright
	&copy;${1}
# ©
snippet £
	&pound;${1}
# £	
